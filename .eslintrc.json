{
    "extends": [
        "react-app"
    ],
    "rules": {
        "quotes": [
            2,
            "single",
            {
                "avoidEscape": true
            }
        ],
        "indent": [
            "error",
            2
        ],
        "semi": [
            2,
            "never"
        ],
        "no-trailing-spaces": "error",
        "eol-last": ["error", "always"]
    }
}